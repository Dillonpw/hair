---
interface Props {
  title: string;
}

const title = Astro.props;
import Header from "../components/Header.astro";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body>
    <Header title={title} />
    <slot />
    <Footer />
  </body>
</html>
